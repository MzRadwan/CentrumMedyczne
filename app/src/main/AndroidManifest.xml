<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.centrummedyczne">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SearchResultActivity"></activity>
        <activity
            android:name=".EditPatientActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_edit_patient"
            android:theme="@style/FullscreenTheme" />
        <activity android:name=".FavDocsActivity" />
        <activity android:name=".MyMedsActivity" />
        <activity android:name=".PrescriptionsActivity" />
        <activity android:name=".PatientAccountActivity" />
        <activity android:name=".AppointmentHistoryActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".PreLoginActivity" />
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>